      "languages": ["latex"],
      "triggers": ["@"],
      "dictionary": "defaults/fonts.json",
      "renderMode": "latex"
    },
    {
      "name": "LaTeX Image Completion",
      "commandName": "latex.image",
      "languages": ["latex"],
      "triggers": [";"],
      "dictionary": "defaults/images.json",
      "renderMode": "latex"
    },
    {
      "name": "LaTeX Image Completion",
      "commandName": "latex.greek",
      "languages": ["latex"],
      "triggers": [":"],
      "dictionary": "defaults/greek.json",
      "renderMode": "latex"
    },
    {
      "name": "Unicode Math",
      "commandName": "text.math",
      "languages": ["markdown", "redtt"],
      "triggers": ["\\"],
      "dictionary": [
        "defaults/math.json",
        { "label": "dim", "body": "dim" },
        { "label": "<:", "body": "<:" },
        { "label": ":>", "body": ":>" },
        { "label": "<|", "body": "<|" },
        { "label": "|>", "body": "|>" }
      ]
    }
